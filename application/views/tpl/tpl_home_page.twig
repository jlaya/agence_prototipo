/* End of file tpl_admin.twig */
/* Location: ./application/controllers/views/twig/template/tpl/tpl_admin.twig */
#}
<!DOCTYPE html>
<head>
    <meta charset="utf-8"/>
    <meta content="width=device-width, initial-scale=1" name="viewport"/>
    {% block head %}
        <title>{{ projectname() }}</title>
        <link rel="canonical" href="https://www.creative-tim.com/product/material-dashboard-pro" />
        <!-- Bootstrap core CSS     -->
        <link href="{{ assets_url()~ 'css/bootstrap.min.css' }}" rel="stylesheet" />
        <!--  Material Dashboard CSS    -->
        <link href="{{ assets_url()~ 'css/material-dashboard.css' }}" rel="stylesheet" />
        <!--  CSS for Demo Purpose, don't include it in your project     -->
        <link href="{{ assets_url()~ 'css/demo.css' }}" rel="stylesheet" />
        <!-- Mult Select -->
        <link href="{{ assets_url()~ 'global/jquery-multi-select/css/multi-select.css' }}" rel="stylesheet" />
        <!--     Fonts and icons     -->
        <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons" />
        <!-- BEGIN GLOBAL MANDATORY STYLES -->
        <style type="text/css" media="screen">
            input[type="text"]::-webkit-input-placeholder {
                text-align: left;
            }
            input[type="text"]::-moz-placeholder { /* Firefox 19+ */
                text-align: left;
            }
            .dropdown-menu{
                min-width: 200px;
            }
        </style>
        {% block stylesheets %}

        {% endblock %}

        <link rel="shortcut icon" href="favicon.ico"/>
    {% endblock %}
</head>
<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo page-md">
   <div class="page-content-wrapper">
    {% block content %}

    {% endblock %}
</div>
</div>
<!--   Core JS Files   -->
<script src="{{ assets_url()~ 'js/jquery-3.1.1.min.js' }}" type="text/javascript"></script>
<script src="{{ assets_url()~ 'js/jquery-ui.min.js' }}" type="text/javascript"></script>
<script src="{{ assets_url()~ 'js/bootstrap.min.js' }}" type="text/javascript"></script>
<script src="{{ assets_url()~ 'js/material.min.js' }}" type="text/javascript"></script>
<script src="{{ assets_url()~ 'global/jquery-multi-select/js/jquery.multi-select.js' }}" type="text/javascript"></script>
<script src="{{ assets_url()~ 'js/perfect-scrollbar.jquery.min.js' }}" type="text/javascript"></script>
<!-- Forms Validations Plugin -->
<script src="{{ assets_url()~ 'js/jquery.validate.min.js' }}"></script>
<!--  Plugin for Date Time Picker and Full Calendar Plugin-->
<script src="{{ assets_url()~ 'js/moment.min.js' }}"></script>
<!--  Charts Plugin -->
<script src="{{ assets_url()~ 'js/chartist.min.js' }}"></script>
<!--  Plugin for the Wizard -->
<script src="{{ assets_url()~ 'js/jquery.bootstrap-wizard.js' }}"></script>


<!-- Vector Map plugin -->
<!-- Sliders Plugin -->
<script src="{{ assets_url()~ 'js/nouislider.min.js' }}"></script>
<!--  Google Maps Plugin    -->

<!-- Select Plugin -->
<script src="{{ assets_url()~ 'js/jquery.select-bootstrap.js' }}"></script>
<!--  DataTables.net Plugin    -->
<script src="{{ assets_url()~ 'js/jquery.datatables.js' }}"></script>
<!-- Sweet Alert 2 plugin -->
<script src="{{ assets_url()~ 'js/sweetalert2.js' }}"></script>
<!--    Plugin for Fileupload, full documentation here: http://www.jasny.net/bootstrap/javascript/#fileinput -->


<!-- Material Dashboard javascript methods -->
<script src="{{ assets_url()~ 'js/material-dashboard.js' }}"></script>
<script src="{{ assets_url()~ 'js/highcharts.js' }}"></script>
<script src="{{ assets_url()~ 'js/exporting.js' }}"></script>
<!-- Material Dashboard DEMO methods, don't include it in your project! -->
<script src="{{ assets_url()~ 'js/demo.js' }}"></script>
<script type="text/javascript">
    $(document).ready(function() {

        // Carga de select multiple
        $("#items_user").multiSelect(), $("#items_user").multiSelect({
            selectableOptgroup: !0
        });
        // Javascript method's body can be found in assets/js/demos.js
        demo.initDashboardPageCharts();


    });
</script>
{% block javascript %}
{% endblock %}

</body>
</html>